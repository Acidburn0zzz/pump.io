<div class="span3 list-menu">
  <ul class="nav nav-tabs nav-stacked">
    <% _.each(data.lists, function(list) { %>
    <li class="<%- ((data.list && data.list.id == list.id) ? 'list active' : 'list') %>">
      <a href="<%- list.url %>"><%- list.displayName %></a>
    </li>
    <% }); %>
  </ul>
</div>
