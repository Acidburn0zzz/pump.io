<ul class="nav nav-tabs nav-stacked" id="list-menu-inner" profile-id="<%- profile.id %>">
  <% _.each(lists, function(list) { %>
  <% listItem = list; %>
  <%= partial("list-menu-item") %>
  <% delete listItem; %>
  <% }); %>
  <% if (user && user.profile.id == profile.id) { %>
  <li class="add-a-list">
    <a href="#" class="new-list">New list...</a>
  </li>
  <% } %>
</ul>
